# A exécuter en ligne de commande:
# Accès à Azure
export ARM_SUBSCRIPTION_ID = $subscriptionId
export ARM_CLIENT_ID = appId
export ARM_CLIENT_SECRET = password
export ARM_TENANT_ID = tenant

# Accès à AWS
export AWS_ACCESS_KEY_ID="enter your access key generated earlier"
export AWS_SECRET_ACCESS_KEY="enter the secret key generated earlier"

